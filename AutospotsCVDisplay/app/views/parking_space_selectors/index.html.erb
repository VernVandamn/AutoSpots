<p id="notice"><%= notice %></p>

<h1>Parking Space Selectors</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_space_selectors.each do |parking_space_selector| %>
      <tr>
        <td><%= parking_space_selector.content %></td>
        <td><%= parking_space_selector.user_id %></td>
        <td><%= link_to 'Show', parking_space_selector %></td>
        <td><%= link_to 'Edit', edit_parking_space_selector_path(parking_space_selector) %></td>
        <td><%= link_to 'Destroy', parking_space_selector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking Space Selector', new_parking_space_selector_path %>
