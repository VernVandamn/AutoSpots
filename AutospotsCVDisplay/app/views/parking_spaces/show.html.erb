<p id="notice"><%= notice %></p>

<p>
  <strong>Parking coords:</strong>
  <%= @parking_space.parking_coords %>
</p>

<p>
  <strong>Image url:</strong>
  <%= @parking_space.image_url %>
</p>

<%= link_to 'Back', parking_spaces_path %>

<div class="container">
  <div class="row" id="main">
  	<div class="col-lg-12 col-sm-12 col-xs-12" >
    	<h1>Autospots Computer Vision</h1>
  		<a title="Results" href="#" id="mainimg">
  			<%= image_tag("#{@parking_space.id}/final.jpg", :class => "thumbnail img-responsive") %>
  		</a>
  	</div>
	</div>
	<hr>

	<% @images.each do |image| %>
		<%= link_to image_tag("spots/#{image.split('/').last}", height:'100px'), "/assets/spots/#{image.split('/').last}", data:{ title:"#{image.split('/').last}" , toggle:'lightbox', gallery:'multiimages' }%>
	<% end %>

	<hr>
		<div class="row" id="main">
	    	<div class="col-lg-12 col-sm-12 col-xs-12" >
		    	<h3>Machine Learning Responses</h3>
      		<%= image_tag("#{@parking_space.id}/output.png", :id => "mainimg", :class => "thumbnail img-responsive") %>
	    	</div>
	    	<div class="col-lg-12 col-sm-12 col-xs-12" >
      		<%= image_tag("#{@parking_space.id}/predictions.jpg", :id => "mainimg", :class => "thumbnail img-responsive") %>
	    	</div>
  	</div>
</div>

